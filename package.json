{
  "name": "@oclif/plugin-legacy",
  "description": "converts older style plugins to be compatible with oclif",
  "version": "1.0.0",
  "author": "Jeff Dickey @jdxcode",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init"
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@heroku-cli/plugin-apps",
      "heroku-status"
    ]
  },
  "bugs": "https://github.com/oclif/plugin-legacy/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.0.0-oclif.2",
    "@oclif/command": "^1.2.23",
    "ansi-escapes": "^3.0.0",
    "debug": "^3.1.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@heroku-cli/plugin-apps": "^2.4.26",
    "@oclif/config": "^1.3.48",
    "@oclif/dev-cli": "^1.0.0",
    "@oclif/errors": "^1.0.1",
    "@oclif/plugin-help": "^1.0.0",
    "@oclif/test": "^1.0.0",
    "@oclif/tslint": "^1.0.1",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.1",
    "@types/semver": "^5.5.0",
    "@types/supports-color": "^3.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "globby": "^8.0.0",
    "heroku-apps": "^2.4.22",
    "heroku-status": "^3.0.10",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/oclif/plugin-legacy",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "oclif/plugin-legacy",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"tsc -p test --noEmit\" \"tslint -p test -t stylish\"",
    "postpublish": "rm .oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest && oclif-dev readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
