{
  "name": "@anycli/plugin-legacy",
  "description": "converts older style plugins to be compatible with anycli",
  "version": "0.0.4",
  "author": "Jeff Dickey @jdxcode",
  "anycli": {
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init"
    },
    "devPlugins": [
      "@anycli/plugin-help",
      "heroku-apps"
    ]
  },
  "bugs": "https://github.com/anycli/plugin-legacy/issues",
  "dependencies": {
    "@anycli/command": "^1.2.13",
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.0.0-anycli.1",
    "ansi-escapes": "^3.0.0",
    "debug": "^3.1.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@anycli/config": "^1.3.23",
    "@anycli/dev-cli": "^0.3.1",
    "@anycli/errors": "^0.2.1",
    "@anycli/plugin-help": "^0.6.8",
    "@anycli/test": "^0.10.11",
    "@anycli/tslint": "^0.2.7",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.1",
    "@types/semver": "^5.5.0",
    "@types/supports-color": "^3.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "globby": "^7.1.1",
    "heroku-apps": "^2.4.22",
    "mocha": "^5.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".anycli.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/anycli/plugin-legacy",
  "keywords": [
    "anycli-plugin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "anycli/plugin-legacy",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"tsc -p test --noEmit\" \"tslint -p test -t stylish\"",
    "postpublish": "rm .anycli.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && anycli-dev manifest && anycli-dev readme",
    "test": "echo mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
